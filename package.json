{
  "name": "frituurapp-payload",
  "version": "1.0.0",
  "description": "An example of a multi tenant application with Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "_dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "migrate": "npm run payload migrate up",
    "build": "npm run migrate && cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:schema": "payload-graphql generate:schema",
    "generate:types": "payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "seed": "npm run payload migrate:fresh",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/lib-storage": "^3.717.0",
    "@payloadcms/db-postgres": "^3.11.0",
    "@payloadcms/next": "^3.11.0",
    "@payloadcms/plugin-cloud-storage": "^3.11.0",
    "@payloadcms/richtext-lexical": "^3.11.0",
    "@payloadcms/storage-s3": "^3.11.0",
    "@payloadcms/translations": "^3.11.0",
    "@payloadcms/ui": "^3.11.0",
    "blurhash": "^2.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "graphql": "^16.9.0",
    "next": "^15.0.0",
    "node-fetch": "^3.3.2",
    "payload": "^3.11.0",
    "qs-esm": "7.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.4.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@payloadcms/graphql": "^3.11.0",
    "@swc/core": "^1.6.13",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.16.2",
    "typescript": "5.5.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
